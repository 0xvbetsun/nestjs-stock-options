{
  "name": "nestjs-stock-options",
  "version": "0.2.0",
  "description": "NestJS module for common operations with stock options",
  "main": "dist/index",
  "types": "dist/index",
  "engines": {
    "node": ">= 10.13.0",
    "npm": ">= 6.11.0"
  },
  "scripts": {
    "build": "rm -rf dist && tsc -p tsconfig.json",
    "format": "prettier --write \"**/*.ts\"",
    "lint": "eslint 'lib/**/*.ts' --fix",
    "test": "cross-env NODE_OPTIONS=--experimental-vm-modules jest",
    "prepublish:npm": "npm run build",
    "publish:npm": "npm publish --access public",
    "prerelease": "npm run build",
    "release": "release-it"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/VBetsun/nestjs-stock-options.git"
  },
  "keywords": [
    "NestJS",
    "stock options"
  ],
  "author": "Vlad Betsun <vlad.betcun@gmail.com>",
  "contributors": [
    {
      "name": "Vlad Betsun",
      "email": "vlad.betcun@gmail.com",
      "author": true
    }
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/VBetsun/nestjs-stock-options/issues"
  },
  "homepage": "https://github.com/VBetsun/nestjs-stock-options#readme",
  "dependencies": {
    "stock-options-calculator": "^1.0.1"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.3.0",
    "@commitlint/config-angular": "^19.1.0",
    "@nestjs/common": "^8.0.6",
    "@nestjs/core": "^8.0.6",
    "@nestjs/testing": "^8.0.6",
    "@types/jest": "^27.0.1",
    "@types/node": "^20.1.3",
    "@typescript-eslint/eslint-plugin": "^7.0.0",
    "@typescript-eslint/parser": "^7.4.0",
    "cross-env": "^7.0.3",
    "eslint": "^8.4.1",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-import": "^2.24.2",
    "husky": "^9.0.11",
    "jest": "^27.1.0",
    "lint-staged": "^15.0.2",
    "prettier": "^3.0.0",
    "reflect-metadata": "^0.1.13",
    "release-it": "^17.0.0",
    "rimraf": "^5.0.0",
    "rxjs": "^7.3.0",
    "ts-jest": "^27.0.0-next.12",
    "typescript": "^4.4.2"
  },
  "peerDependencies": {
    "@nestjs/common": "^6.0.0 || ^7.0.0 || ^8.0.0",
    "@nestjs/core": "^6.0.0 || ^7.0.0 || ^8.0.0",
    "reflect-metadata": "^0.1.12",
    "rxjs": "^6.0.0 || ^7.3.0"
  },
  "lint-staged": {
    "*.ts": [
      "prettier --write"
    ]
  },
  "jest": {
    "extensionsToTreatAsEsm": [
      ".ts"
    ],
    "globals": {
      "ts-jest": {
        "useESM": true
      }
    },
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "testEnvironment": "node",
    "rootDir": "lib",
    "testRegex": ".spec.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "coverageDirectory": "./coverage"
  }
}
